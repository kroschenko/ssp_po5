<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="202.0" prefWidth="354.0" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.spp.labs.lab10_spp.SecondaryController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <ComboBox fx:id="computerCombo" prefHeight="26.0" prefWidth="402.0" promptText="Выберите компьютер" visibleRowCount="3" />
      <ComboBox fx:id="userCombo" prefHeight="26.0" prefWidth="402.0" promptText="Выберите пользователя" visibleRowCount="3" />
      <DatePicker fx:id="regDateInput" prefHeight="26.0" prefWidth="402.0" promptText="Выберите дату регистрации" />
      <HBox prefHeight="26.0" prefWidth="199.0">
         <children>
            <Button fx:id="submitButton" alignment="CENTER" mnemonicParsing="false" prefHeight="26.0" prefWidth="402.0" text="Принять" />
         </children>
      </HBox>
   </children>
</VBox>
